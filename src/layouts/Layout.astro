---
import BaseLayout from './BaseLayout.astro';

export interface Props {
	title: string;
  description?: string;
  image?: string;
  ogImage?: {
    template: string;
    data?: Record<string, any>;
  }
}

const { title, description, image, ogImage } = Astro.props;
---

<BaseLayout title={title} description={description} image={image} ogImage={ogImage} >
  <slot />
</BaseLayout>
		


